{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://skillcrafter.onrender.com/api/$1"
    },
    {
      "source": "/auth/(.*)",
      "destination": "https://skillcrafter.onrender.com/auth/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
